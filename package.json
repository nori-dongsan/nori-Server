{
  "name": "nori-Server",
  "version": "1.0.0",
  "description": "nori-Server with Express",
  "main": "index.js",
  "repository": "https://github.com/nori-dongsan/nori-Server.git",
  "author": "Crayon <lsh328328@naver.com>",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --watch 'src/**/*' --exec 'ts-node' src/server.ts",
    "build:tsc": "npm run build:routes && npm run build:swagger && tsc --build",
    "start": "ts-node src/server.ts",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "build:swagger": "tsoa swagger && mkdir -p dist && cp ./src/swagger.json ./dist/swagger.json",
    "build:routes": "tsoa routes"
  },
  "dependencies": {
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "body-parser": "^1.20.0",
    "class-validator": "^0.13.2",
    "express": "^4.18.1",
    "inversify": "^6.0.1",
    "inversify-binding-decorators": "^4.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.19",
    "swagger-ui-express": "^4.4.0",
    "tsoa": "^4.1.0",
    "typeorm": "^0.2.41",
    "winston": "^3.8.1",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.2",
    "@types/swagger-ui-express": "^4.1.3",
    "concurrently": "^7.2.2",
    "ts-node": "^10.8.2"
  }
}
